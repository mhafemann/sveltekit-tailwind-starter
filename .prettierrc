{
	"trailingComma": "es5",
	"tabWidth": 2,
	"printWidth": 80,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"useTabs": false,
	"jsxSingleQuote": false,
	"bracketSameLine": true,
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "preserve",
	"tailwindConfig": "./tailwind.config.cjs",
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
